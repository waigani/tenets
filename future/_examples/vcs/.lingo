# find func who's got extra params from last pr
tenets:
  - bots:
      codelingo/clair:
        name: growing-args
        doc: "funcs should not take more than three args"
        comment: "The arg list to this func is getting quite long. It should instead take a dataset."
    query: |
      @ clair.comment
      func:
        git.commit: "HEAD^"
          param:
            count: p
        param:
          count: > $p


